<?php
// data kelas dengan array 2 dimensi
$array = array(
    "10" => array("udin", "ismail", "adi"),
    "10" => array("Lukman", "fajri", "mahmud")
);

// menampilkan data array
print_r($array);

// menapilkan kelas 1C
print_r($array['1D']);

// menampilkan kelas 1d dengan index 8
echo $array['1D'][0];

// tampilkan fajri
// tampilkan andi

// data kelas bisa dituliis juga dengan
$array_simple = [
    "10" => ["udin", "ismail", "adi"],
    "10" => ["Lukman", "fajri", "mahmud"]
];